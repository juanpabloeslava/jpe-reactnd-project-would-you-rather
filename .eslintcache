her/src/components/Poll.js",["82"],"import React, { Component } from 'react'\nimport { formatDate } from '../utils/helpers'\nimport PollOptions from './PollOptions'\nimport { withRouter } from 'react-router-dom'\n// material imports\nimport { Button } from '@material-ui/core';\n\nclass Poll extends Component {\n\n    state = {\n        alreadyAnswered: false,\n        answer: ''\n    }\n\n    showResult = (answer)  => {\n        this.setState(() => {\n            return { \n                alreadyAnswered: true,\n                answer\n            };\n        });\n    }\n\n    toHome = () => {\n        // By using withRouter() down by connect(), it passes history to the comp as props even though React Router is not rendering the comp. \n        this.props.history.push(`/`)\n    }\n\n    render() {\n\n        const { alreadyAnswered, answer } = this.state\n\n        return (\n            <div className='list-container'>\n                <h3 className='center'>Poll</h3>\n                <div>\n                    <div className='poll'>\n                        <div className='avatar-container'>\n                            <img className='avatar' src={`https://gravatar.com/avatar/90e832b87dc32a1741b5e30afd452824?s=400&d=robohash&r=x`} />\n                        </div>\n                        <div className='poll-info'>\n                            <div>\n                                <span className='user-name'> Sarah asked</span>\n                                <div> {formatDate(1468479767190)} </div>\n                                <p> Would you rather: </p>\n                                <div className='poll-options'>\n                                    <PollOptions \n                                        showResult={this.showResult}\n                                        optionOne='have horrible short term memory'\n                                        optionTwo='have horrible long term memory'/>\n                                </div>\n                                { alreadyAnswered && (\n                                <div>\n                                    {/* <p> You chose: </p> */}\n                                    <span className='submission-summary'>You would rather {answer}</span>\n                                    <div className='poll-btns'>\n                                        <div>\n                                            <Button onClick={ () => this.toHome()} className='MuiButton-contained'>Back to Polls</Button>\n                                        </div>\n                                        <div>\n                                            <Button className='MuiButton-containedPrimary'>Next Poll</Button>\n                                        </div>\n                                    </div>\n                                </div>\n                                )}\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n}\n\n\nexport default withRouter(Poll)\n","/Users/juan.eslava/Desktop/JP Personal/ES PC/00 - courses/React Nanodegree/0 - Projects/Would you rather/jpe-reactnd-project-would-you-rather/src/components/PollOptions.js",[],"/Users/juan.eslava/Desktop/JP Personal/ES PC/00 - courses/React Nanodegree/0 - Projects/Would you rather/jpe-reactnd-project-would-you-rather/src/utils/helpers.js",[],"/Users/juan.eslava/Desktop/JP Personal/ES PC/00 - courses/React Nanodegree/0 - Projects/Would you rather/jpe-reactnd-project-would-you-rather/src/components/PollPercentage.js",[],"/Users/juan.eslava/Desktop/JP Personal/ES PC/00 - courses/React Nanodegree/0 - Projects/Would you rather/jpe-reactnd-project-would-you-rather/src/views/PollView.js",[],"/Users/juan.eslava/Desktop/JP Personal/ES PC/00 - courses/React Nanodegree/0 - Projects/Would you rather/jpe-reactnd-project-would-you-rather/src/views/AddPoll.js",["83","84","85"],"/Users/juan.eslava/Desktop/JP Personal/ES 