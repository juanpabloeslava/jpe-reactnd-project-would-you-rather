Poll)\n","/Users/juan.eslava/Desktop/JP Personal/ES PC/00 - courses/React Nanodegree/0 - Projects/Would you rather/jpe-reactnd-project-would-you-rather/src/components/PollOptions.js",[],"/Users/juan.eslava/Desktop/JP Personal/ES PC/00 - courses/React Nanodegree/0 - Projects/Would you rather/jpe-reactnd-project-would-you-rather/src/utils/helpers.js",[],"/Users/juan.eslava/Desktop/JP Personal/ES PC/00 - courses/React Nanodegree/0 - Projects/Would you rather/jpe-reactnd-project-would-you-rather/src/components/PollPercentage.js",[],"/Users/juan.eslava/Desktop/JP Personal/ES PC/00 - courses/React Nanodegree/0 - Projects/Would you rather/jpe-reactnd-project-would-you-rather/src/views/PollView.js",[],"/Users/juan.eslava/Desktop/JP Personal/ES PC/00 - courses/React Nanodegree/0 - Projects/Would you rather/jpe-reactnd-project-would-you-rather/src/views/AddPoll.js",["71","72","73"],"/Users/juan.eslava/Desktop/JP Personal/ES PC/00 - courses/React Nanodegree/0 - Projects/Would you rather/jpe-reactnd-project-would-you-rather/src/components/AddPollOptions.js",[],"/Users/juan.eslava/Desktop/JP Personal/ES PC/00 - courses/React Nanodegree/0 - Projects/Would you rather/jpe-reactnd-project-would-you-rather/src/components/NavBar.js",[],"/Users/juan.eslava/Desktop/JP Personal/ES PC/00 - courses/React Nanodegree/0 - Projects/Would you rather/jpe-reactnd-project-would-you-rather/src/views/Login.js",[],"/Users/juan.eslava/Desktop/JP Personal/ES PC/00 - courses/React Nanodegree/0 - Projects/Would you rather/jpe-reactnd-project-would-you-rather/src/actions/actionTypes.js",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":39,"column":29,"nodeType":"80","endLine":39,"endColumn":145},{"ruleId":"81","severity":1,"message":"82","line":5,"column":10,"nodeType":"83","messageId":"84","endLine":5,"endColumn":16},{"ruleId":"81","severity":1,"message":"85","line":31,"column":17,"nodeType":"83","messageId":"84","endLine":31,"endColu